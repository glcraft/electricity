{
  "name": "electricity",
  "version": "1.0.0",
  "description": "Electron Explorer",
  "main": "app-dist/app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "watch-app": "tsc -w -p app",
    "watch-renderer": "tsc -w -p renderer",
    "watch": "npm-run-all --parallel watch-app watch-renderer",
    "rebuild": "electron-rebuild -f -w internal_module",
    "postinstall": "npm run rebuild"
  },
  "keywords": [
    "explorer"
  ],
  "author": "GLCraft",
  "license": "ISC",
  "dependencies": {
    "@types/pug": "^2.0.4",
    "bindings": "^1.5.0",
    "electron": "^8.0.0",
    "internal_module": "file:./my-modules/internal_module",
    "pug": "^2.0.4",
    "subpackage": "^1.1.0",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "electron-rebuild": "^1.10.0",
    "nan": "^2.14.0",
    "node-abi": "^2.14.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^3.7.5"
  },
  "subPackages": []
}
